<!DOCTYPE html>
<html>
	<head></head>
	<body>
		<h1>thumbnail</h1>
		<form action="/upload" method="post" enctype="multipart/form-data">

			<input type="file" name="uploadImg">
			<input type="submit">

		</form> 

		<div class="thumbnail-list">
			<% files.forEach(function(file){ %>
				<img src="../thumbnails/<%=file%>">
			<%})%>
		</div>
	</body>
</html>